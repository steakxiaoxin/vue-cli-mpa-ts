{
  "name": "vue-cli-mpa-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build",
    "serve": "vue-cli-service serve",
    "precommit": "yarn lint",
    "commit": "git add -A && git-cz",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"",
    "build:dev": "cross-env NODE_ENV=production APP_ENV=dev yarn build",
    "build:prd": "cross-env NODE_ENV=production APP_ENV=prd yarn build",
    "build:pre": "cross-env NODE_ENV=production APP_ENV=pre yarn build",
    "build:stage": "cross-env NODE_ENV=production APP_ENV=stage yarn build",
    "serve:dev": "cross-env NODE_ENV=development APP_ENV=dev yarn serve",
    "serve:prd": "cross-env NODE_ENV=development APP_ENV=prd yarn serve",
    "serve:pre": "cross-env NODE_ENV=development APP_ENV=pre yarn serve",
    "serve:stage": " cross-env NODE_ENV=development APP_ENV=stage yarn serve"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "dayjs": "^1.8.28",
    "element-ui": "^2.13.2",
    "lodash": "^4.17.15",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-plugin-component": "^1.1.1",
    "chalk": "^4.0.0",
    "commander": "^5.1.0",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "simple-progress-webpack-plugin": "^1.1.2",
    "terser-webpack-plugin": "^3.0.2",
    "typescript": "~3.9.3",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
