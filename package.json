{
  "name": "vue-cli-mpa-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build",
    "serve": "vue-cli-service serve",
    "precommit": "yarn lint",
    "commit": "git add -A && git-cz",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"",
    "build:dev": "cross-env NODE_ENV=production APP_ENV=dev yarn build",
    "build:prd": "cross-env NODE_ENV=production APP_ENV=prd yarn build",
    "build:pre": "cross-env NODE_ENV=production APP_ENV=pre yarn build",
    "build:stage": "cross-env NODE_ENV=production APP_ENV=stage yarn build",
    "serve:dev": "cross-env NODE_ENV=development APP_ENV=dev yarn serve",
    "serve:prd": "cross-env NODE_ENV=development APP_ENV=prd yarn serve",
    "serve:pre": "cross-env NODE_ENV=development APP_ENV=pre yarn serve",
    "serve:stage": " cross-env NODE_ENV=development APP_ENV=stage yarn serve"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^2.6.11",
    "dayjs": "^1.8.24",
    "element-ui": "^2.13.1",
    "lodash": "^4.17.15",
    "simple-progress-webpack-plugin": "^1.1.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-plugin-typescript": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "commitizen": "^4.0.4",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^4.2.5",
    "prettier": "^1.18.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "typescript": "3.5.3",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
